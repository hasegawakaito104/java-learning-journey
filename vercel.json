{
  "version": 2,
  "name": "java-learning-journey-banking",
  "builds": [
    {
      "src": "BankingProject/Portfolio/simple-bank-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "buildCommand": "cd BankingProject/Portfolio/simple-bank-frontend && npm run build"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/BankingProject/Portfolio/simple-bank-frontend/build/index.html"
    }
  ]
}